{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "overrides": [
    {
      "files": ["*.stories.jsx", "**/stories/**"],
      "rules": {
        "import/no-extraneous-dependencies": 0
      }
    }
  ]
}
